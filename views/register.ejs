<div class="row align-items-center" style="height: 100vh;">
<div class="container mx-auto col-10 col-md-8 col-lg-6 bg-light">
<form>
    <!--Header-->
    <div class="logo row text-center">
        <p>VONGO</p>
    </div>
    <div class="row mx-auto justify-content-center">
        <div class="form-outline mb-4 text-center col-md-4 col-lg-3 p-0 mr-1">
            <input type="username" id="form2Example3" class="form-control" />
            <label class="form-label m-1" for="form2Example2">Username</label>
          </div>
          <div class="form-outline mb-4 text-center col-md-4 col-lg-3 p-0 ml-1">
            <input type="username" id="form2Example3" class="form-control" />
            <label class="form-label m-1" for="form2Example2">Location</label>
          </div>
    </div>
    
    <!-- Email input -->
    <div class="form-outline mb-4 mx-auto text-center col-6">
      <input type="email" id="form2Example1" class="form-control" />
      <label class="form-label m-1" for="form2Example1">Email address</label>
    </div>
  
    <!-- Password input -->
    <div class="form-outline mb-4 mx-auto text-center col-6">
      <input type="password" id="form2Example2" class="form-control" />
      <label class="form-label m-1" for="form2Example2">Password</label>
    </div>
  
    <!-- 2 column grid layout for inline styling -->
    <div class="row mx-auto text-center justify-content-center">
    <!-- Submit button -->
    <button type="button" class="btn btn-primary btn-block mb-4 text-center">Register</button>
    </div>
    <!-- Register buttons -->
    <div class="text-center">
      <p>Already a member? <a href="./login">Login</a></p>
    </div>
  </form>
